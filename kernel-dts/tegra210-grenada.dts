/dts-v1/;

/memreserve/ 0x80000000 0x00020000;

#include "tegra210.dtsi"
#include "tegra210-platforms/tegra210-ers-dfll-e2174-1101-a00.dtsi"
#include "tegra210-platforms/tegra210-grenada-fixed.dtsi"
#include "tegra210-platforms/tegra210-comms.dtsi"
#include <dt-bindings/pinctrl/pinctrl-tegra.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "grenada";
	compatible = "nvidia,grenada", "nvidia,tegra210";

	chosen {
		bootargs ="mem=1024M@2048M console=ttyS0,115200 earlyprintk=uart8250-32bit,0x70006000";
        };

	/* chosen */

	memory@80000000 {
		device_type = "memory";
		reg = < 0x0 0x80000000 0x0 0x40000000 >;
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts = <1 13 0xf01
			      1 14 0xf01
			      1 11 0xf01
			      1 10 0xf01>;
		clock-frequency = <38400000>;
	};

	i2c@7000c000 {
		rt5639: audio-codec@1c {
			compatible = "realtek,rt5639";
			reg = <0x1c>;
		};
	};

	adma@702e2000  {
		interrupts = <0 24 0x04
			      0 25 0x04
			      0 26 0x04
			      0 27 0x04
			      0 28 0x04
			      0 29 0x04
			      0 30 0x04
			      0 31 0x04
			      0 32 0x04
			      0 33 0x04>;
		dma-channels = <10>;
		status = "okay";
	};

	spi@7000d600 {
		status = "okay";
		spi-max-frequency = <25000000>;
	};

	spi@70410000 {
		status = "okay";
		spi@0 {
			compatible = "spidev";
			reg = <0>;
			spi-max-frequency = <13000000>;

			controller-data {
				nvidia,x1-len-limit = <16>;
				nvidia,x1-bus-speed = <50>; /* In Mhz */
				nvidia,x1-dymmy-cycle = <0>;
				nvidia,x4-bus-speed = <80>;
				nvidia,x4-dymmy-cycle = <8>;
			};

		};
	};

	serial@70006000 {
		compatible = "nvidia,tegra210-uart";
		console-port;
		status = "okay";
	};

	serial@70006040 {
		compatible = "nvidia,tegra210-uart";
		status = "okay";
	};

	serial@70006200 {
		status = "okay";
	};

