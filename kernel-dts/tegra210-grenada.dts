/dts-v1/;

/include/ "tegra210.dtsi"

/ {
	model = "grenada";
	compatible = "nvidia,grenada", "nvidia,tegra210";

	chosen {
		bootargs ="mem=1024M@2048M vmalloc=192M console=ttyS0,2400 lpj=2482176 androidboot.console=ttyS0 earlyprintk=uart8250-32bit,0x70006000";
        };

	/* chosen */

	memory@80000000 {
		device_type = "memory";
		reg = < 0x0 0x80000000 0x0 0x40000000 >;
	};

	ahub {
		status = "okay";

		ope@702d8000 {
			status = "okay";

			peq@702d8100 {
				status = "okay";
			};
		};

		ope@702d8400 {
			status = "okay";

			peq@702d8500 {
				status = "okay";
			};
		};

		sfc@702d2000 {
			status = "okay";
		};

		sfc@702d2200 {
			status = "okay";
		};

		sfc@702d2400 {
			status = "okay";
		};

		sfc@702d2600 {
			status = "okay";
		};

		spkprot@702d8c00 {
			status = "okay";
		};
	};
};
